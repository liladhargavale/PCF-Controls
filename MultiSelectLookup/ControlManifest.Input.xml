<?xml version="1.0" encoding="utf-8" ?>
<manifest>
  <control namespace="LookupMultiSelect" constructor="LookupMultiSelect" version="9.6.0" display-name-key="LookupMultiSelect_Control" description-key="LookupMultiSelect_Description" control-type="standard">
    <external-service-usage enabled="false"></external-service-usage>
      <property name="selectedItems" display-name-key="selectedItems" description-key="selectedItemsDescription" of-type="Lookup.Simple" required="true" usage="bound">
      </property>

      <property name="hiddenField" display-name-key="hiddenField" description-key="hiddenFieldDescription" of-type="Multiple" usage="bound" required="true">
      </property>

      <property name="recordNamesField" display-name-key="recordNamesField" description-key="recordNamesFieldDescription" of-type="Multiple" usage="bound" required="false">
      </property>

      <property name="LookupEntityLogicalName" display-name-key="lookupEntityLogicalName" description-key="selectedItemsDescription" of-type="SingleLine.Text" required="true" usage="input"></property>

      <property name="LookupEntityDisplayName" display-name-key="lookupEntityDisplayName" description-key="Display name of the lookup entity" of-type="SingleLine.Text" required="false" usage="input"></property>

      <property name="NameFieldLogicalName" display-name-key="NameFieldLogicalName" description-key="selectedItemsDescription" of-type="SingleLine.Text" required="true" usage="input">
      </property>

      <property name="LookupOption1LogicalName" display-name-key="LookupOption1LogicalName" description-key="selectedItemsDescription" of-type="SingleLine.Text" required="false" usage="input">
      </property>

      <property name="LookupOption2LogicalName" display-name-key="LookupOption2LogicalName" description-key="selectedItemsDescription" of-type="SingleLine.Text" required="false" usage="input">
      </property>

    <resources>
        <code path="index.ts" order="1"/>
        <css path="css/MultiSelectLookup.css" order="1" />
    </resources>

    <feature-usage>
      <!-- <uses-feature name="Device.captureAudio" required="true" />
      <uses-feature name="Device.captureImage" required="true" />
      <uses-feature name="Device.captureVideo" required="true" />
      <uses-feature name="Device.getBarcodeValue" required="true" />
      <uses-feature name="Device.getCurrentPosition" required="true" />
      <uses-feature name="Device.pickFile" required="true" /> -->
      <uses-feature name="Utility" required="true" />
      <uses-feature name="WebAPI" required="true" />
    </feature-usage>
  </control>
</manifest>
